      \strng{namehash}{6d3525fd969c2161db8ffc3c397ea8a2}
      \strng{fullhash}{6d3525fd969c2161db8ffc3c397ea8a2}
      \strng{bibnamehash}{6d3525fd969c2161db8ffc3c397ea8a2}
      \strng{authorbibnamehash}{6d3525fd969c2161db8ffc3c397ea8a2}
