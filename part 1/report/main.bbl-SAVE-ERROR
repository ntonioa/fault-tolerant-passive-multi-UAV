      \strng{namehash}{5f953427fd24e84792a6c0237330938f}
      \strng{fullhash}{5f953427fd24e84792a6c0237330938f}
      \strng{bibnamehash}{5f953427fd24e84792a6c0237330938f}
      \strng{authorbibnamehash}{5f953427fd24e84792a6c0237330938f}
